<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>canvas</title>
</head>
<body>
    <canvas id="canvas4" width="400" height="300"></canvas>
    <script>
        let canvas = document.getElementById("canvas4");
        let ctx = canvas.getContext("2d");

        ctx.fillStyle = "yellow";
        ctx.fillRect(100, 150, 150, 100);

        ctx.fillStyle = "blue";
        ctx.fillRect(130, 170, 30, 30);
        ctx.fillRect(190, 170, 30, 30);

        ctx.beginPath();
        ctx.moveTo(100, 150);
        ctx.lineTo(175, 80);
        ctx.lineTo(250, 150);
        ctx.closePath();
        ctx.fillStyle = "brown";
        ctx.fill();
    </script>
</body>
</html>
